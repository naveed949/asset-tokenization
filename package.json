{
  "name": "Tokenization_System",
  "version": "1.0.1",
  "description": "This project contains contracts of a System of asset tokenization",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "./node_modules/.bin/truffle compile",
    "_deploy": "./node_modules/.bin/truffle deploy --network ",
