    "deploy-mainnet": "npm run _deploy mainnet",
    "deploy-rinkeby-reset": "./node_modules/.bin/truffle deploy --network rinkeby --reset",
    "test": "./node_modules/.bin/truffle test"
  },
  "license": "Apache-2",
  "devDependencies": {
    "ganache-cli": "^6.1.0",
    "truffle": "5.1.14",
    "truffle-hdwallet-provider": "1.0.5"
  },
  "dependencies": {
    "bn.js": "^4.11.8",
    "dotenv": "^6.0.0",
    "openzeppelin-solidity": "^2.1.3",
    "solc": "^0.5.1",
    "truffle-assertions": "^0.9.2"
  }
}
